        <sharedLight filename="lights/wallLight25w.xml" linkNode="linkedLight12" groupIndex="1"/>
        <sharedLight filename="lights/wallLight25w.xml" linkNode="linkedLight13" groupIndex="1"/>
        <sharedLight filename="lights/wallLight25w.xml" linkNode="linkedLight14" groupIndex="1"/>
        <sharedLight filename="lights/billboardLight25w.xml" linkNode="linkedLight15" groupIndex="1"/>
        <sharedLight filename="lights/billboardLight25w.xml" linkNode="linkedLight16" groupIndex="1"/>
        <sharedLight filename="lights/billboardLight25w.xml" linkNode="linkedLight17" groupIndex="1"/>
        <sharedLight filename="lights/billboardLight25w.xml" linkNode="linkedLight18" groupIndex="1"/>
        <sharedLight filename="lights/billboardLight25w.xml" linkNode="linkedLight19" groupIndex="1"/>
        <sharedLight filename="lights/billboardLight25w.xml" linkNode="linkedLight20" groupIndex="1"/>
        <sharedLight filename="lights/billboardLight25w.xml" linkNode="linkedLight21" groupIndex="1"/>
        <sharedLight filename="lights/billboardLight25w.xml" linkNode="linkedLight22" groupIndex="1"/>
        <sharedLight filename="lights/billboardLight25w.xml" linkNode="linkedLight23" groupIndex="1"/>
        <sharedLight filename="lights/billboardLight25w.xml" linkNode="linkedLight24" groupIndex="1"/>
        <sharedLight filename="lights/wallLight25w.xml" linkNode="linkedLight25" groupIndex="1"/>
        <sharedLight filename="lights/wallLight25w.xml" linkNode="linkedLight26" groupIndex="1"/>        
        <realLights>
            <low>
                <light node="realLightsLow" groupIndex="1"/>
            </low>
            <high>
                <light node="realLightsHigh" groupIndex="1"/>
            </high>
        </realLights>
    </lights>

