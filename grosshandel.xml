<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<placeable type="sellingStation" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../shared/xml/schema/placeable.xsd">
    <storeData>
        <name>
			<de>Großhandel</de>
			<en>Wholesale</en>
		</name>
        <functions>
            <function>
				<de>Hier können alle Landwirtschaftlichen Produkte verkauft werden.</de>
				<en>All agricultural products can be sold here.</en>
			</function>
        </functions>
        <image>store_grosshandel.png</image>
        <price>90000</price>
        <lifetime>1000</lifetime>
        <rotation>0</rotation>
        <brand>NONE</brand>
        <species>placeable</species>
        <category>productionPoints</category>
        <brush>
            <type>placeable</type>
            <category>production</category>
            <tab>sellingPoints</tab>
        </brush>
        <vertexBufferMemoryUsage>552960</vertexBufferMemoryUsage>
        <indexBufferMemoryUsage>61184</indexBufferMemoryUsage>
        <textureMemoryUsage>6553600</textureMemoryUsage>
        <audioMemoryUsage>0</audioMemoryUsage>
        <instanceVertexBufferMemoryUsage>0</instanceVertexBufferMemoryUsage>
        <instanceIndexBufferMemoryUsage>0</instanceIndexBufferMemoryUsage>
    </storeData>

    <base>
        <filename>Grosshandel.i3d</filename>
        <canBeRenamed>true</canBeRenamed>
    </base>

    <placement useRandomYRotation="false" useManualYRotation="true" >
        <testAreas>
            <testArea startNode="testArea1Start" endNode="testArea1End" />
        </testAreas>	
		<sounds>
			<place template="smallImp" />
		</sounds>
    </placement>

    <clearAreas>
        <clearArea startNode="clearArea1Start" widthNode="clearArea1Width" heightNode="clearArea1Height"/>
        <clearArea startNode="clearArea2Start" widthNode="clearArea2Width" heightNode="clearArea2Height"/>
    </clearAreas>

    <leveling requireLeveling="true" maxSmoothDistance="10" maxSlope="75" maxEdgeAngle="30" >
        <levelAreas>
            <levelArea startNode="levelArea1Start" widthNode="levelArea1Width" heightNode="levelArea1Height" groundType="asphalt"/>
            <levelArea startNode="levelArea2Start" widthNode="levelArea2Width" heightNode="levelArea2Height" groundType="asphalt"/>
        </levelAreas>
    </leveling>

    <indoorAreas>
        <indoorArea startNode="indoorArea1Start" widthNode="indoorArea1Width" heightNode="indoorArea1Height" />
		<indoorArea startNode="indoorArea2Start" widthNode="indoorArea2Width" heightNode="indoorArea2Height" />
    </indoorAreas>

    <tipOcclusionUpdateAreas>
        <tipOcclusionUpdateArea startNode="tipOcclusionUpdateArea1Start" endNode="tipOcclusionUpdateArea1End" />
    </tipOcclusionUpdateAreas>

    <ai>
        <updateAreas>
            <updateArea startNode="tipOcclusionUpdateArea1Start" endNode="tipOcclusionUpdateArea1End" />
        </updateAreas>
		<splines>
            <spline node="aiSpline1" />
        </splines>
    </ai>

    <hotspots>
        <hotspot type="PRODUCTION_POINT" linkNode="unloadTriggerMarker" teleportNode="unloadTriggerMarker" />
    </hotspots>

    <infoTrigger triggerNode="infoTrigger" />

    <dynamicallyLoadedParts>
        <dynamicallyLoadedPart filename="props/props.i3d" node="0" linkNode="propsLinkNode" />
        <dynamicallyLoadedPart filename="$data/placeables/sellingStationGeneric/sellingStationGenericNoCover.i3d" node="0" linkNode="0>12" />
    </dynamicallyLoadedParts>

	<sellingStation node="sellingStation" supportsExtension="false">
		<unloadTrigger fillTypes="WOOD" aiNode="unloadTriggerAINode">
            <woodTrigger triggerNode="woodTrigger" activationTriggerNode="woodSellTrigger" />
        </unloadTrigger>
		<unloadTrigger fillTypes="WOOD" aiNode="unloadTriggerAINode">
			<woodTrigger triggerNode="containerTrigger" class="PalletUnloadTrigger" />
		</unloadTrigger>
		<unloadTrigger exactFillRootNode="unloadTrigger"  aiNode="unloadTriggerAINode" fillTypeCategories="BULK LIQUID PIECE WINDROW COMBINE SPRAYER SPREADER MIXERWAGON FARMSILO HAYLOFT PRODUCT VEGETABLES">
			<baleTrigger triggerNode="baleTrigger" deleteLitersPerSecond="5000" />
			<palletTriggerNode triggerNode="palletTrigger" />
			<fillTypeCategorie name="BULK"            	priceScale="1" supportsGreatDemand="true" disablePriceDrop="false" />
			<fillTypeCategorie name="LIQUID"         	priceScale="1" supportsGreatDemand="true" disablePriceDrop="false" />
			<fillTypeCategorie name="PIECE"         	priceScale="1" supportsGreatDemand="true" disablePriceDrop="false" />
			<fillTypeCategorie name="WINDROW"           priceScale="1" supportsGreatDemand="true" disablePriceDrop="false" />
			<fillTypeCategorie name="COMBINE"         	priceScale="1" supportsGreatDemand="true" disablePriceDrop="false" />
			<fillTypeCategorie name="SPRAYER"         	priceScale="1" supportsGreatDemand="true" disablePriceDrop="false" />
			<fillTypeCategorie name="SPREADER"          priceScale="1" supportsGreatDemand="true" disablePriceDrop="false" />
			<fillTypeCategorie name="MIXERWAGON"        priceScale="1" supportsGreatDemand="true" disablePriceDrop="false" />
			<fillTypeCategorie name="FARMSILO"         	priceScale="1" supportsGreatDemand="true" disablePriceDrop="false" />
			<fillTypeCategorie name="HAYLOFT"           priceScale="1" supportsGreatDemand="true" disablePriceDrop="false" />
			<fillTypeCategorie name="PRODUCT"         	priceScale="1" supportsGreatDemand="true" disablePriceDrop="false" />
			<fillTypeCategorie name="VEGETABLES"        priceScale="1" supportsGreatDemand="true" disablePriceDrop="false" />
		</unloadTrigger>
	</sellingStation>

	<triggerMarkers>
        <triggerMarker node="unloadTriggerMarker" filename="$data/shared/assets/marker/markerIcons.xml" id="UNLOAD" adjustToGround="false" />
        <triggerMarker node="woodSellTriggerMarker" filename="$data/shared/assets/marker/markerIcons.xml" id="EXCLAMATION_MARK" adjustToGround="true" />
    </triggerMarkers>

    <lights>
        <group weatherRequiredFlags="NIGHT" />

        <sharedLight filename="lights/wallLight25w.xml" linkNode="linkedLight1" groupIndex="1"/>
        <sharedLight filename="lights/wallLight25w.xml" linkNode="linkedLight2" groupIndex="1"/>
        <sharedLight filename="lights/wallLight25w.xml" linkNode="linkedLight3" groupIndex="1"/>
        <sharedLight filename="lights/wallLight25w.xml" linkNode="linkedLight4" groupIndex="1"/>
